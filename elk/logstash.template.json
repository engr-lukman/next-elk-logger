{
  "template": "logstash",
  "index_patterns": ["ats-logstash-*"],
  "settings": {
    "number_of_shards": 1
  },
  "mappings": {
    "properties": {
      "lavel": {
        "type": "keyword"
      },
      "message": {
        "type": "text"
      },
      "@timestamp": {
        "type": "date"
      }
    }
  }
}
